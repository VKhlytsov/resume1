<div class="row">
{{#each footer}}
<div class="col-2">
<ul class="nav flex-column">
{{#each this}}
{{#if @last}}
<li class="nav-item">
<a class="nav-link disabled text-sm-start" aria-current="page" href="{{link}}">{{text}}</a>
</li>
{{else}}
<li class="nav-item">
<a class="nav-link active text-sm-start" aria-current="page" href="{{link}}">{{text}}</a>
</li>
{{/if}}
{{/each}}
</ul>
</div>
{{/each}}
</div>