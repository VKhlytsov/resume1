<main>

<p class="fs-1 fw-semibold me-2">{{header}}</p>

<div>
{{#with purchase}}
<ul class="nav nav-tabs">
{{#each tabs}}  
{{#if @first}}
<li class="nav-item">
<a class="nav-link active fw-semibold" aria-current="page" href="#">{{this}}</a>
</li>
{{/if}}
{{#if @last}}
<li class="nav-item">
<a class="nav-link" href="#">{{this}}</a>
</li>
{{/if}}
{{/each}}
{{/with}}
</div>

<div class="row mt-3 mb-5">
<div class="col-8 position-absolute g-1">  
{{#with purchase.actionBlock.input}}
<div class="col-12 input-group position-absolute mb-3">
<input type="text" class="form-control rounded m-2 mb-3" placeholder="{{placeholder}}">
{{/with}}
{{#with  purchase.actionBlock}}
{{#each buttons}}
{{#if isSuccess}}
<button href="{{link}}" class="input-group-text btn btn-success fw-semibold rounded m-2 mb-3" type="button">{{text}}</button>
{{/if}}
{{#if isOutline}}
<button href="{{link}}" class="btn btn-outline-success fw-semibold rounded m-2 mb-3" type="button">{{text}}</button>
{{/if}}
{{#if isDanger}}
<button href="{{link}}" class="btn btn-outline-danger fw-semibold rounded m-2 mb-3" type="button">{{text}}</button>
{{/if}}
{{/each}}
</div>
{{/with}}
</div>
</div>
<div class="border-bottom pb-3"></div>


=================================

<div class="row">
<div class="col-12 mt-4 mb-4">  

<div class="card mb-4">
<div class="row gy-2">
<div class="col-4 p-3 pe-3">
{{#with purchase}}
{{#each itemList}}
<p class="fs-6">{{number}}</p>
<h5 class="text py-1">{{status}}</h5>
{{/each}}
{{/with}}
</div>
<div class="col-2">
<div class="card-body">
{{#with purchase}}
{{#each itemList}}
{{#with amount}}
<p class="card-title">{{title}}:</p>
<h5 class="card-text py-2">{{value}}</h5>
{{/with}}
{{/each}}
{{/with}}
</div>
</div>

<div class="col-md-4 p-4">
{{#with purchase}}
{{#each itemList}}
{{#each images}}

<img src="{{this}}" class="img m-2">

{{/each}}
{{/each}}
{{/with}}
<div class="card-body">
</div>
</div>

<div class="col-2 p-2">
{{#with purchase}}
{{#each itemList}}
{{#with button}}
<div class="card-body">
{{#if isPrimary}}
<a href="{{link}}" class="btn btn-primary">{{text}}</a>
{{/if}}
</div>
{{/with}}
{{/each}}
{{/with}}
</div> 
</div>
</div>
</div>
</div>
<div class="border-bottom pb-3"></div>

