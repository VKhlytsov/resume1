<h2 class="text-end">{{title_list.sub}}</h2>
<button type="button" class="btn btn-outline-info btn-lg">{{button.text}}</button>
{{/with}}
</div>
</div>
<br>
<div class="card">
<div class="card-body">
{{#with section.advantage}}
