<h6 class="text-start text-end">{{sub_title}}</h6>
<p class="fs-6 text-start text-end">{{description}}</p>
<button type="button" class="btn btn-outline-warning btn-sm">{{button}}</button>
{{/with}} 
</div>
</div>
</div>
</div>
{{/with}} 
<br>
<div class="card">
<div class="card-body">
{{#with section.agreement}}
<p class="display-5 fw-normal">{{title}}</p>
<div class="card">
<div class="card-body">
{{#with info}}
<h5 class="text-start">{{sub_title}}</h5>
<br>
<div class="card">
<div class="card-body">
<p class="fs-6 text-start">{{detail.text}}</p>
<button type="button" class="btn btn-success"> {{detail.button}} </button>
