<p class="fs-6 text-start">{{description}}</p>
<button type="button" class="btn btn-outline-warning btn-lg">{{button}}</button>
{{/with}}
</div>
</div>
<br>
<div class="card">
<div class="card-body">
{{#with innovation}}
<p class="display-4 fw-normal text-muted text-center">{{title}}</p>
<h6 class="text-start text-center">{{sub_title}}</h6>
<p class="fs-6 text-start text-center">{{description}}</p>
<button type="button" class="btn btn-outline-warning btn-md">{{button}}</button>
{{/with}} 
</div>
</div>
<br>
<div class="card">
<div class="card-body">
{{#with collaboration}}
