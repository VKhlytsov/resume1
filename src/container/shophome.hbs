<div class="card m-2">  
{{#each links}}           
<div class="nav">
{{#if href}}    
{{#if @last}}
<li class="nav-item"> 
<a class="nav-link disabled" href="{{href}}">{{text}}</a>
</li> 
{{else}}
<li class="nav-item">
<a class="nav-link active" href="{{href}}">{{text}}</a>
{{/if}}
{{/if}}
{{/each}}
</li>
</div>
</div>
{{/with}}

{{>shoppost}}

{{#with goodsBlock}}
<div class="col-12">
<div class="row">
<ol class="nav nav-tabs">
{{#each tabs}}   
{{#if isActive}}
{{#if isDisabled}}
<li class="nav-item active">
<a class="nav-link disabled" href="#" >{{text}}</a>
</li>
{{else}}
<li class="nav-item active">
<a class="nav-link active" href="#" >{{text}}</a>
</li>
{{/if}}
{{else}}
{{#unless isActive}}
{{#if isDisabled}}
<li class="nav-item">
<a class="nav-link disabled" href="#" >{{text}}</a>
</li>
{{else}}
<li class="nav-item disabled">
<a class="nav-link" href="#" >{{text}}</a>
</li>
{{/if}}
{{/unless}}
{{/if}}
{{/each}} 
</ol>
</div>
</div>
</div>
{{/with}}
{{#with goodsBlock}}
<section>
<div class="row g-3">
{{#each cards}}
<div class="col-4">   
</div>
{{/each}}
</div>
</section>
{{/with}}
============
{{#with subscribe}}
<div class="card text-bg-dark mb-4">
<div class="card-boby">
<h2 class="text-start m-2 mb-3">{{header}}</h2>
<p class="fs-6 text-start m-2">{{description}}</p>
{{#each buttons}}
<div class="col-3 m-2 mb-2">
<a class="btn btn-primary" href="{{link}}">{{text}}</a>
</div>
{{/each}}
</div>
</div>
{{/with}}
=============
{{#with service}}
<div class="card text-bg-success text-center">
<div class="card-body">
<h2 class="card-title">{{title}}</h2>
<p class="card-text">{{description}}</p>
{{#each buttons}}
<a class="btn btn-primary btn-md" type="button" href="{{link}}">{{text}}</a>
{{/each}}
{{/with}}
</div>
</div>   
