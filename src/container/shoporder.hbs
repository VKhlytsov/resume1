<main>




<div>
<p class="fs-1 fw-semibold me-2">{{header}}</p>
<br>
</div>

<div class="row mx-2">

<div class="col-8"> 
<div class="row ps-1">
{{#with contacts_data}}
<p class="fs-2 fw-semibold mb-3">{{title}}</p>
{{/with}}
</div>
<div class="row">
{{#with contacts_data}}
{{#each buttons}}
<div class="col-6">
{{#if isPrimary}}
<button type="button" class="btn btn-warning text-light px-4 mt-1">{{text}}</button> 
{{/if}}
{{#if isOutline}}
<button type="button" class="btn btn-outline-warning px-4 mt-1">{{text}}</button>
{{/if}}
</div>
{{/each}}
{{/with}}
</div>  



<section>
<div class="card mt-2">
<div class="card-body">
{{#each action_block}}
<div class="col-4">
{{#if isSuccess}} 
<button type="button" class="btn btn-success text-light mb-2">{{text}}</button>
{{/if}}
{{#if isDanger}} 
<button type="button" class="btn btn-danger text-light mb-2">{{text}}</button>
{{/if}}
{{#if isInfo}} 
<button type="button" class="btn btn-info text-light">{{text}}</button>
{{/if}}
</div>
{{/each}}
</div>
</div>

<div class="card">
<div class="card-body">
{{#with total}}
<div class="col-12">
<p class="fs-2 fw-semibold me-1">{{title}}:</p>
{{#each item_list}}
<p class="fs-5 mb-1">{{name}}: {{price}}</p>
{{/each}}
<hr>
{{#each additional_info}}
<p class="fs-5 mb-1">{{text}}: {{value}}</p>
{{/each}}
<hr>
{{#with total_price}}
<p class="fs-5 mb-1">{{text}}: <strong>{{value}}</strong></p>
{{/with}}
<hr>
{{#each button}}
{{#if isPrimary}}
<button type="button" class="btn btn-primary text-light mb-2">{{text}}</button>
{{/if}}
{{#if isOutline}}
<button type="button" class="btn btn-outline-primary mb-2">{{text}}</button>
{{/if}}
{{/each}}
</div>
{{/with}}
</div>
</div>
</section>


<section>
<div class="row px-1">
{{#with contacts_data.form.input}}
{{#with surname}}
<div class="col-5">
<div class="mb-3">
<label for="inputLabel" class="form-label">{{label}}</label>
<div class="input-group">
<span class="input-text" id="First name" aria-label="First name"></span>
<input type="text" class="form-control" placeholder="{{placeholder}}" id="First name" aria-label="First name">
</div>
<div class="form-text" id="Help">{{caption}}</div>
</div>
{{/with}}
<div class="col-8">
{{#with name}}
<div class="mb-3">
<label for="inputLabel" class="form-label">{{label}}</label>
<div class="input-group">
<span class="input-text" id="Last name" aria-label="Last name"></span>
<input type="text" class="form-control" placeholder="{{placeholder}}" id="Last name" aria-label="Last name">
</div>
<div class="form-text" id="Help">{{caption}}</div>
{{/with}}
</div>
</div>
</div>
{{#with phone}}
<div class="col-5">
<div class="mb-3">
<label for="inputLabel" class="form-label">{{label}}</label>
<div class="input-group">
<span class="input-text" id="phone" aria-label="phone"></span>
<input type="text" class="form-control" placeholder="{{placeholder}}" id="phone" aria-label="phone">
</div>
<div class="form-text" id="Help">{{caption}}</div>
</div>
{{/with}}
{{#with email}}
<div class="mb-3">
<label for="inputLabel" class="form-label">{{label}}</label>
<div class="input-group">
<span class="input-text" id="email" aria-label="email"></span>
<input type="text" class="form-control" placeholder="{{placeholder}}" id="email" aria-label="email">
</div>
<div class="form-text" id="Help">{{caption}}</div>
{{/with}}
</div>
</div>
{{/with}}
</section>
<hr>

<section>
<div class="row px-2 ms-1">
{{#with contacts_data.form}}
{{#each switch}}
<div class="form-check form-switch">
{{#if isActive}}
<input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckDefault" checked>
<label class="form-check-label" for="flexSwitchCheckDefault">{{text}}</label>
</div>
<div class="form-check form-switch">
{{else}}
<input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckChecked">
<label class="form-check-label" for="flexSwitchCheckChecked">{{text}}</label>
</div>
{{/if}}
{{/each}}
</div>
{{/with}}  
</section>
<hr>

<section>
<div class="row">
<h3 class="fs-3 fw-semibold me-2">{{delivery.title}}</h3>
<div class="card">
<div class="col-7 row ps-1 pt-1 pb-0 mb-0">
<div class="card-body">
{{#each delivery.select}}
<div class="col-8 mb-1 pb-1">
<label class="form-label">{{label}}</label>
<select class="form-select" aria-label="Доставка">
<option select>{{placeholder}}</option>
{{#each options}}
<option value="{{value}}">{{text}}</option>
{{/each}}
</select>
</div>
{{/each}}
</div>
</div>
</div>
</div>
</section>

<section>
<div class="row pt-1 mt-0 mb-2 ps-0">
{{#each delivery.delivery_type}}
<div class="form-check">
<input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault2" checked>
<label class="form-check-label" for="flexRadioDefault2">{{type}}</label>
</div>
{{/each}}
</div>
<hr>
</section>


<section>
<h3 class="fs-3 fw-semibold me-2">{{delivery.payment.title}}</h3>
{{#each delivery.payment.list}}
<div class="form-check">
<label class="form-check-label">{{type}}  
{{#if description}} 
<div id="Help" class="form-text">{{description}}</div>
{{/if}}
</label>
<input class="form-check-input" name="type" type="radio">
</div>
{{/each}}
<hr>
</section>
           

<section>
<div class="row">
{{#with delivery.contact}}
<h3 class="fs-3 fw-semibold me-2">{{title}}</h3>
<div class="row">
{{#with surname}}
<div class="col-5 py-2">
<label for="inputLabel" class="form-label">{{label}}</label>
<input type="text" class="form-control" placeholder="{{placeholder}}" aria-label="First name">
<div id="Help" class="form-text">{{caption}}</div>
</div>
{{/with}}
{{#with name}}
<div class="col-5 py-2">
<label for="inputLabel" class="form-label">{{label}}</label>
<input type="text" class="form-control" placeholder="{{placeholder}}" aria-label="Last name">
<div id="Help" class="form-text">{{caption}}</div>
</div>
{{/with}}
</div>
<div class="row ps-0 ms-0">
{{#with phone}}
<div class="col-5 py-2">
<label for="inputLabel" class="form-label">{{label}}</label>
<input type="text" class="form-control" placeholder="{{placeholder}}" aria-label="Phone">
<div id="Help" class="form-text">{{caption}}</div>
</div>
{{/with}}
{{#with email}}
<div class="col-5 py-2">
<label for="inputLabel" class="form-label">{{label}}</label>
<input type="text" class="form-control" placeholder="{{placeholder}}" aria-label="E-mail">
<div id="Help" class="form-text">{{caption}}</div>
</div>
{{/with}}
{{/with}}
</div>
</div>
<hr>
</section>


<section>
<div class="row pb-2 ps-2">
<div class="row"> 
{{#with delivery.coment}}
<label for="inputLabel" class="form-label">{{label}}</label>
<textarea class="form-control" id="exampleFormControlTextarea1" rows="1" placeholder="{{placeholder}}"></textarea>
{{/with}}       
</div>     
</div>
<hr>
</section>

<section>
<div class="row">
{{#with coupon}}
<div class="row pb-2">
<h3 class="fs-3 fw-semibold me-3">{{title}}</h3>
{{#with select.availabel_coupon}}
<label for="inputLabel" class="form-label">{{label}}</label>
<select class="form-select form-select-lg mb-2" aria-label=".form-select-lg example">
<option selected>{{placeholder}}</option>
{{#each options}}
<option value="{{value}}">{{text}}</option>
{{/each}}
</select>
{{/with}}
</div>
{{#with select.friend_coupon}}
<div class="row pb-2">
<label for="inputLabel" class="form-label">{{label}}</label>
<select class="form-select form-select-sm" aria-label=".form-select-sm example">
<option selected>{{placeholder}}</option>
{{#each options}}
<option value="{{value}}">{{text}}</option>
{{/each}}
</select>
<div class="form-text" id="help">Caption text, description, error notification</div>
</div>
{{/with}}
{{/with}}
</div>
<div class="row pb-1 mt-3">
{{#with coupon.add}}
<div class="row pb-1">
{{#with friend}}
<label for="inputLabel" class="form-label">{{label}}</label>
<input class="form-control form-control-lg" type="text" value="{{placeholder}}" placeholder=".form-control-lg" aria-label=".form-control-lg example">
<div id="Help" class="form-text">{{caption}}</div>
{{/with}}
</div>
{{/with}}
</div>
<div class="row pb-1 mt-2">
{{#with coupon.add}}
<div class="row pb-1">
{{#with colleague}}
<label for="inputLabel" class="form-label">{{label}}</label>
<input class="form-control form-control-sm" type="text" value="{{placeholder}}" placeholder=".form-control-sm" aria-label=".form-control-sm example">
<div id="Help" class="form-text">{{caption}}</div>
{{/with}}
</div>
{{/with}}
</div>
<hr>
</section>

</div>
<section>
<div class="row ps-2 py-2">
{{#with quiz}}
<h3 class="fs-3 fw-semibold me-3">{{title}}</h3>
{{#with preference}}
<h6>{{title}}</h6>
{{#each options}}
<div class="form-check">
<input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault2" checked>
<label class="form-check-label" for="flexRadioDefault2">{{this}}</label>
</div>
{{/each}}
{{/with}}
</div>
<div class="row ps-2 py-2">
<label for="FormControlInput" class="form-label">{{your_expirience.label}}</label>
<input type="email" class="form-control" id="exampleFormControlInput1" placeholder="">
</div>
<div class="row ps-2 py-2">
<label for="FormControlInput" class="form-label">{{purchase_frequency.label}}</label>
<select class="form-select" aria-label="Default select example">
<option selected>{{purchase_frequency.placeholder}}</option>
{{#with purchase_frequency}}
{{#each options}}
<option value="{{value}}">{{text}}</option>
{{/each}}
{{/with}}
</select>
</div>
<div class="row ps-2 py-2">
<label for="FormControlInput" class="form-label">{{factors.title}}</label>
{{#with factors}}
{{#each options}}
<div class="form-check">
<input class="form-check-input" name="type" type="checkbox">
<label class="form-check-label">{{this}}</label>
</div>
{{/each}}
{{/with}}
</select>
</div>
{{/with}}
</section>




</div>






















</main>