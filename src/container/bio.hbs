<main>
<h2> <ins>{{name}}</ins></h2>
<h3><b>Specialized at:</b></h3>
    {{#each known_for}}
    <li>{{this}}</li>
    {{/each}}
    <hr>
    <b>Occupation:</b> - <i> ({{occupation}})</i>
    <br>
    <b>Nationality:</b> - <i> ({{nationality}})</i>
    <br>
    <hr>
    <ins><b>Born:</b></ins> <i>{{birthdate}}</i> - {{birthplace}}
    <br>
    <ins><b>Death:</b></ins> <i>{{deathdate}}</i> - {{deathplace}}
    <hr>
    <h3><b>Education :</b></h3>
   {{#each education}}
    <li>Degree - {{degree}}</li>
    <li>Field - {{field}}</li>
    <li>Institute - {{institution}}</li>
    <li>Graduation year - {{year}}</li>
    {{/each}}
    <hr>
    <h3>Notable publications :</h3>
    {{#each notable_publications}}
    <b>Publisher -</b> <ins>{{this.publisher}}</ins>
    <li>Title - {{this.title}}</li>
    <li>Year - {{this.year}}</li>
    {{/each}}
    <hr>
    <h3>Awards :</h3>
    {{#each awards}}
    <b>Title -</b> <ins>{{this.title}}</ins>
    <li>Description - {{this.description}}</li>
    <li>Year - {{this.year}}</li>
    {{/each}}
    <hr>
    <h3>Partners :</h3>
    {{#each partners}}
    <b>Name - </b> <ins>{{this.name}}</ins>
    <li>Relationship - {{this.relationship}}</li>
    <li>Year - {{this.years}}</li>
    {{/each}}
    <hr>
    <h3>Influences :</h3>
    {{#each influences}}
    <li><b>Name - </b><ins>{{this}}</ins></li>
    {{/each}}
    <h3>Influenced :</h3>
    {{#each influenced}}
    <li><b>Name - </b><ins>{{this}}</ins></li>
    {{/each}}
    <hr>
    <h3><ins>Quotes : </ins></h3>
    {{#each quotes}}
    <q>{{this}}</q>
    {{/each}}
    <hr>
    <h3>Major discoveries :</h3>
    {{#each major_discoveries}}
    <b>Title - </b> {{this.title}}
    <br>
    <b>Year - </b> {{this.year}}
    <br>
    <b>Description - </b> {{this.description}}
    {{/each}}
    <hr>
    <h3>Contributions :</h3>
    {{#with contributions}}
    <b>Title - </b> {{title}}
    <br>
    <b>Field - </b> {{field}}
    <br>
    <b>Year - </b> {{year}}
    <br>
    <b>Description : </b> {{description}}    
    {{/with}}
    <hr>
</main>